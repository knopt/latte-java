class Node {
  int value;
  Node next;

  void setNext(Node n) {
    self.next = n;
  }

  void setValue(int n) {
    self.value = n;
  }

  Node getNext() {
    return self.next;
  }

  int getValue() {
    return self.value;
  }

}


int main() {
  Node n1 = new Node;
  Node n2 = new Node;

  n2.setValue(5);
  n2.setNext(null);

  

  return 0;
}
